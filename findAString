def count_substring(string, sub_string):
    lst = []
    count = 0
    for i in range(0,len(string)-len(sub_string)+1):
        lst.append(string[i:i+len(sub_string)])

    for i in lst:
        if i == sub_string:
            count += 1          
    return(count)
        


