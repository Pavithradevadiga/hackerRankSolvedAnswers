# Enter your code here. Read input from STDIN. Print output to STDOUT
n,m = map(int,input().split())
arr = []
happiness = 0
arr = list(map(int,input().split(' ')))
p = set(list(map(int,input().split(' '))))
q = set(list(map(int,input().split(' '))))

for i in arr:
    if i in p:
        happiness += 1
    if i in q:
        happiness -= 1     
        
print(happiness)        

        
